package kr.or.ddit.basic;

import java.util.ArrayList;
import java.util.HashSet;
import java.util.List;
import java.util.Random;
import java.util.Scanner;

public class LottoMain {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("몇개 구매하시겠습니까? ");
		int numOfLottery = Integer.parseInt(sc.nextLine());
		System.out.println("자동으로 몇개 생성하시겠습니까? ");
		int numOfAuto = Integer.parseInt(sc.nextLine());
		
		for (int i = 0; i < numOfLottery; i++) {
			HashSet<Integer> lotterySet = new HashSet<Integer>();
			while (lotterySet.size() < 6) {
				lotterySet.add(new Random().nextInt(45) + 1);
			}
			System.out.println(lotterySet);
		}
		
		
		
	}

}
